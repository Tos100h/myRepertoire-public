        <table class="table table-striped table-hover custom-table">
            <thead class="text-center">
                <tr>
                    <th class="hide-on-mobile">Link</th>
                    <th>Song name</th>
                    <th>Media name</th>
                </tr>
            </thead>
            <tbody>
                {% for m in media %}
                    <tr class="text-center">
                        <td class="col-4 hide-on-mobile">
                            {{ include('common/_media_icon.html.twig', {'media': m}) }}
                        </td>
                        <td class="col-4">
                            <a href="{{ path('app_song_show', {'id': m.song.id}) }}">
                                <strong>{{ m.song.name }}</strong>
                            </a>
                        </td>
                        <td class="col-4">
                            {{ m.name }}
                        </td>
                    </tr>
                {% else %}
                    <tr>
                        <td colspan="12" class="text-center"><strong>No records found</strong></td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>